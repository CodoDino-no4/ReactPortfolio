    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "husky": "4.0.7",
    "lint-staged": "^13.0.2",
    "prettier": "^2.5.1",
    "react-hot-loader": "^4.13.0",
    "sass": "^1.45.1",
    "sass-loader": "10.1.1",
    "style-loader": "0.23.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.2"
