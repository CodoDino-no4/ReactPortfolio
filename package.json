    "react-dom": "yarn:@hot-loader/react-dom",
    "react-intersection-observer": "^8.33.1",
    "react-router-dom": "^6.2.1",
    "react-scroll": "^1.8.4",
    "styled-components": "^5.3.3",
    "webpack": "5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/eslint-parser": "^7.16.5",
